#set("body") {
    #if(uuid != "unknown") {
        <h2>Welcome!</h2>
        <p>Please remember your unique code: #(uuid)</p>
        <p><a href="/" class="btn btn-success">Login</a></p>
    }

    #if(errorMessage != "noError") {
        <h2>Oh ooh!</h2>
        #(errorMessage)
    }
}

#embed("master")